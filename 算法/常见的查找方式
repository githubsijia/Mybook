###顺序查找
int sequence(int *a，int n，int x)
{
  int i;
  for(i=0;i<10;i++)
  {
   if(a[i]==x)
    return i;
  }
  return -1;
}

###折半查找
int BinSearch(int* a, int n, int x)
{
	int mid;
	int low = 0;
	int high = n - 1;
	while (low<=high)
	{
		mid = (high + low) / 2;
		if (x > a[mid])
		{
			low = mid + 1;
		}
		else if (x < a[mid])
		{
			high = mid - 1;
		}
		else
		{
			return mid;
		}
	}
	return -1;
}
